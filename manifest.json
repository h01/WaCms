{
	"manifest_version": 2,
	"name": "哇Cms!",
	"version": "1.0",
	"description": "扫描网站所用管理系统和第三方库",
	"homepage_url": "https://github.com/h01/WaCms",
	"browser_action": {
		"default_icon": "images/icon_48.png",
		"default_title": "WaCms!",
		"default_popup": "popup.html"
	},
	"icons": {
		"128": "images/icon_128.png",
		"48": "images/icon_48.png",
		"16": "images/icon_16.png"
	},
	"background": {
		"scripts": ["js/background.js"]
	},
	"permissions": ["notifications", "contextMenus", "tabs", "*://*/*"]
}
